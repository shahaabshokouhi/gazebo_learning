        <model name="Jetracer" canonical_link="chassis">
            <pose relative_to="world">-8.0 0 0 0 0 0</pose>
            
            <frame name="lidar_frame" attached_to="chassis">
                <pose>0.8 0 0.5 0 0 0</pose>
